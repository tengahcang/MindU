{
  "expo": {
    "name": "MindU",
    "slug": "MindU",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/iconnew.png",
    "userInterfaceStyle": "light",
    "scheme": "my-scheme",
    "splash": {
      "image": "./assets/splashh.png",
      "resizeMode": "cover",
      "backgroundColor": "#006494"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/iconnew.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.wahy.MindU"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
